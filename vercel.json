{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "maxDuration": 10
    },
    "api/analyze.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/analyze",
      "destination": "/api/analyze"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}